{
  "name": "bpmnlint-plugin-camunda",
  "version": "0.6.1",
  "description": "Camunda modeling guidelines, packed as a bpmnlint plug-in",
  "main": "index.js",
  "scripts": {
    "all": "run-s lint test",
    "lint": "eslint .",
    "dev": "npm test -- --watch",
    "test": "mocha test/**/*-spec.js",
    "generate-docs-images": "npx @bpmn-io/bpmnlint-generate-docs-images ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/camunda/bpmnlint-plugin-camunda.git"
  },
  "keywords": [
    "camunda",
    "bpmn",
    "bpmnlint",
    "lint-rules",
    "bpmnlint-plugin"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "license": "MIT",
  "devDependencies": {
    "@bpmn-io/bpmnlint-generate-docs-images": "^0.3.2",
    "bpmn-moddle": "^8.0.1",
    "bpmnlint": "^8.2.0",
    "camunda-bpmn-moddle": "^7.0.1",
    "chai": "^4.3.4",
    "eslint": "^9.39.1",
    "eslint-plugin-bpmn-io": "^2.2.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "bpmnlint-utils": "^1.0.2"
  },
  "files": [
    "rules",
    "index.js"
  ]
