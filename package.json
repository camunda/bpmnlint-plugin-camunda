{
  "name": "bpmnlint-plugin-camunda",
  "version": "0.4.0",
  "description": "Camunda modeling guidelines, packed as a bpmnlint plug-in",
  "main": "index.js",
  "scripts": {
    "all": "run-s lint test",
    "lint": "eslint .",
    "dev": "npm test -- --watch",
    "test": "mocha 'test/**/*.js'",
    "generate-docs-images": "npx @bpmn-io/bpmnlint-generate-docs-images ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/camunda/bpmnlint-plugin-camunda.git"
  },
  "keywords": [
    "camunda",
    "bpmn",
    "bpmnlint",
    "lint-rules",
    "bpmnlint-plugin"
  ],
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "license": "MIT",
  "devDependencies": {
    "@bpmn-io/bpmnlint-generate-docs-images": "^0.2.0",
    "bpmnlint": "^6.1.1",
    "chai": "^4.1.2",
    "eslint": "^6.3.0",
    "eslint-plugin-bpmn-io": "^0.10.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "bpmnlint-utils": "^1.0.2"
  }
}
